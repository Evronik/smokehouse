<script type="text/javascript">
    $(document).ready(function() {

  /* считываем, записываем в куки utm метки. Если метка больше предыдущей, сохраняем ее */
  var work_utm = "optovikam"; 
  var len_cur_utm = work_utm.length;
  var len_ex_utm = $.cookie('utm');

  if($.cookie('utm')!=null && len_cur_utm < len_ex_utm.length){
    var utm = $.cookie('utm');
  } else {
    var utm = "optovikam";
  }
  if(utm!='' && utm!='/') {
    $.cookie('utm', utm);   
  }
  $('.pole-utm').val($.cookie('utm'));
  /* считываем, записываем в куки utm метки. Если метка больше предыдущей, сохраняем ее */
  });

</script>



